digraph fst {
	graph [pad="0.5", nodesep="1", ranksep="2"];
	rankdir=LR;
	size="10,10";
	node [shape = point]; start;
	node [shape = doublecircle];
		"0, ulɑ";
		"1, ulɑ";
		"2, olɑ";

	"0, ulɑ" ->	"0, ulɑ" [ label = "?:?" ];
	"0, ulɑ" ->	"1, ulɑ" [ label = "u:u" ];
	"0, ulɑ" ->	"2, olɑ" [ label = "o:o" ];
	"2, olɑ" ->	"1, ulɑ" [ label = "u:u" ];
	"1, ulɑ" ->	"2, olɑ" [ label = "o:o" ];
	"1, ulɑ" ->	"1, ulɑ" [ label = "?:?" ];
	"2, olɑ" ->	"2, olɑ" [ label = "?:?" ];
	start -> "0, ulɑ";
}