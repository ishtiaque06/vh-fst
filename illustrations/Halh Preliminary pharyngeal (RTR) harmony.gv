digraph fst {
	graph [pad="0.5", nodesep="1", ranksep="2"];
	rankdir=LR;
	size="10,10";
	node [shape = point]; start;
	node [shape = doublecircle];
		"0, λ";
		"1, λ";
		"2, λ";

	"0, λ" ->	"0, λ" [ label = "?:?" ];
	"1, λ" ->	"1, λ" [ label = "?:?, i:i, e:e, u:u, o:o, ʊ:u, a:e, ɔ:o" ];
	"2, λ" ->	"2, λ" [ label = "?:?, a:a, ʊ:ʊ, ɔ:ɔ, u:ʊ, e:a, o:ɔ, i:i" ];
	"0, λ" ->	"1, λ" [ label = "i:i, e:e, u:u, o:o" ];
	"0, λ" ->	"2, λ" [ label = "a:a, ʊ:ʊ, ɔ:ɔ" ];
	start -> "0, λ";
}