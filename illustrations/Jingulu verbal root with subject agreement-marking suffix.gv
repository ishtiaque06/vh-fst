digraph fst {
	graph [pad="0.5", nodesep="1", ranksep="2"];
	rankdir=LR;
	size="10,10";
	node [shape = point]; start;
	node [shape = doublecircle];
		"0, λ";
		"1, λ";

	"0, λ" ->	"0, λ" [ label = "?:?, a:i" ];
	"0, λ" ->	"1, λ" [ label = "i:i, u:u" ];
	"1, λ" ->	"1, λ" [ label = "?:?, a:a, i:i, u:u" ];
	start -> "0, λ";
}